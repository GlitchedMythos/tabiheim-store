{
  "name": "server",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "prebuild": "cpy \"**\" \"./public\" --cwd=../web/build/client",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "predeploy": "cpy \"**\" \"./public\" --cwd=../web/build/client",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "hono": "^4.10.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250115.0",
    "cpy-cli": "^5.0.0",
    "wrangler": "^4.45.2"
  }
}
